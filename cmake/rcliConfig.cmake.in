include(FindPackageHandleStandardArgs)
set(${CMAKE_FIND_PACKAGE_NAME}_CONFIG ${CMAKE_CURRENT_LIST_FILE})
find_package_handle_standard_args(rcli CONFIG_MODE)

if(NOT TARGET rcli::rcli)
    include("${CMAKE_CURRENT_LIST_DIR}/rcliTargets.cmake")
    if((NOT TARGET rcli) AND
       (NOT RCLI_FIND_VERSION OR
        RCLI_FIND_VERSION VERSION_LESS 1.5.0))
        add_library(rcli INTERFACE IMPORTED)
        set_target_properties(rcli PROPERTIES
            INTERFACE_LINK_LIBRARIES rcli::rcli
        )
    endif()
endif()
